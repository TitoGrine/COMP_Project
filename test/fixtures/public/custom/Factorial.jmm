import static io.println(int) void;

class Factorial {

    public static void main(String[] args) {
        Factorial factorial;
        int num;
        int fact;


        factorial = new Factorial();
        num = 10;
        fact = factorial.multiplyNumbers(num);

        io.println(fact);
    }

    public int multiplyNumbers(int num) {
        int result;

        if(num < 1) {
            result = 1;
        }
        else {
            result = num * this.multiplyNumbers(num - 1);
        }

        return result;
    }
}
