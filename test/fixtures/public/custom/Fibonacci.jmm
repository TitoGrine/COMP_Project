import static io.println(int) void;

class Fibonacci {

    public static void main(String[] args) {
        Fibonacci fibonacci;
        int fibo;

        fibonacci = new Fibonacci();

        fibo = fibonacci.fib(10);
        io.println(fibo);
    }

    public int fib(int n) {
      int result;

      if (!(n < 0) && !(0 < n))
         result = n;
      else if (!(n < 1) && !(1 < n))
        result = n;
      else
         result = this.fib(n - 1) + this.fib(n - 2);

      return result;
    }
}
