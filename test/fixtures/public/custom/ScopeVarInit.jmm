    public int testing(int w) {
        int test_variable;

        x = 3;


        while(x < 5){
            test_variable = 1;
            x = x + 1;
        }

        y = test_variable * 2 + 1; // Suppose to fail since test_variable might not be initialized

        if(y < x){
            y = y - x;

            while(z){
                test_variable = 3;
                z = false;
            }
        } else {
            test_variable = y + x * 7;
        }

        z = test_variable < x; // Suppose to fail since test_variable might not be initialized

        if(y < x){
            while(x - 2 < y){
                y = y + 1;
                if(z){
                    test_variable = y * y / 3;
                } else {
                    z = true;
                }
            }
        } else{
            if(z)
                test_variable = x + y + w;
            else
                test_variable = y + x * 7;
        }


        z = test_variable < y + x && w < x; // Suppose to fail since test_variable might not be initialized

        if(z)
            test_variable = 0 - 1;
        else {
            if(y < x - 2){
                if(3 < x && x + w < 0){
                    test_variable = 1;
                } else {
