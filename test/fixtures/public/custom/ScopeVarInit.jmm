    public int testing(int w) {
        int test_variable;

        x = 3;

        while(x < 5){
            test_variable = 1;
            x = x + 1;
        }

        y = test_variable * 2 + 1; // Suppose to fail since test_variable might not be initialized

        if(y < x){
            y = y - x;
        } else {
