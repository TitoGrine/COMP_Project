        }

        z = test_variable < x; // Suppose to fail since test_variable might not be initialized

        if(z){
            test_variable = 0 - 1;
        } else {
            test_variable = 1;
        }

        return test_variable; // Doesn't fail since it's guaranteed to be initialized
    }
