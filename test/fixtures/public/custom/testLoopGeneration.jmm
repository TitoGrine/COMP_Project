import static io.println(int);
class LoopGen {

	int[] jk;

	public static void main(String[] args) {
		int a;
		int i;
		int j;
		boolean sum_complete;

		a = 1;
		i = 1;
		jk = new int[10];
        jk[0] = a;

		while(i < jk.length){
		    j = 0;
		    a = 0;

		    sum_complete = false;
		    while(!sum_complete){
		        a = a + jk[j];
		        sum_complete = j < i;
		    }

		    jk[i] = a;
		}

        io.println(jk[jk.length - 1]);
	}
}