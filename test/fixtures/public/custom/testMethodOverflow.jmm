import io;
import static io.println(int);

class Overflow {

	public static void main(String[] args) {
        Overflow simple;
        int a;
        int b;
        int result1;
        int result2;
        int result3;

        simple = new Overflow();
        a = 3;
        b = 5;

        result1 = simple.add(a, simple);
        result2 = simple.add(b, new io());

        result3 = simple.add(result1, result2);

		io.println(result1);
		io.println(result2);
		io.println(result3);
	}

	public int add(int a, int b) {
		return a+b;
	}

	public int add(int a, Overflow b) {
		return a-20;
	}

	public int add(int a, io b) {
		return a+15;
	}
}